class Solution:
    def subtractProductAndSum(self, n: int) -> int:
        n=list(map(int,list(str(n))))
        p=1
        for k in n:
            p*=k
        return p-sum(n)
    """approach-2
        p=1
        s=0
        while n>0:
            a=n%10
            p*=a
            s+=a
            n=n//10
        return p-s"""
